{
  "global": {
    "data": "data",
    "audit": "audit",
    "instance": 1,
    "host": "localhost"
  },

  "admin": {
    "process": {
      "pm2": {
        "library": "randomSeed,setPort,hopen,bus.client,heartbeat.client,berror.client",
        "arg": {
          "setPort": "%basePort% + 1"
        }
      },
      "bus": {
        "library": "randomSeed,setPort,hopen,bus.server,heartbeat.client,berror.client",
        "arg": {
          "setPort": "%basePort% + 2"
        }
      },
      "tick": {
        "library": "randomSeed,setPort,hopen,bus.client,tick.stream,tick.hft,monitor.cep.server,heartbeat.server,usage.server,berror.server,error.server",
        "arg": {
          "tick": "hft",
          "setPort": "%basePort% + 3"
        }
      },
      "replay": {
        "library": "randomSeed,setPort,hopen,bus.client,tick.replay,heartbeat.client,berror.client",
        "arg": {
          "setPort": "%basePort% + 4"
        }
      },
      "rdb": {
        "library": "randomSeed,setPort,hopen,bus.client,tick.sub,heartbeat.client,berror.client",
        "arg": {
          "setPort": "%basePort% + 5"
        }
      },
      "cdb": {
        "library": "randomSeed,setPort,hopen,bus.client,tick.cdb,heartbeat.client,berror.client",
        "arg": {
          "setPort": "%basePort% + 6"
        }
      },
      "ctp":{
        "library": "randomSeed,setPort,hopen,bus.client,tick.sub,tick.cep,tick.ctp,websocket,heartbeat.client,berror.client,error.client",
        "arg": {
          "setPort": "%basePort% + 7"
        }        
      },
      "monitorCep":{
        "library": "randomSeed,setPort,hopen,bus.client,tick.sub,tick.cep,monitor.cep.client,heartbeat.client,berror.client,error.client",
        "arg": {
          "setPort": "%basePort% + 8"
        }        
      }      
    }
  },

  "backfill": {
    "process": {

      "bus": {
        "library": "randomSeed,setPort,hopen,bus.server,heartbeat.client,berror.client",
        "arg": {
          "setPort": "%basePort% + 1"
        }
      },

      "rdb": {
        "library": "randomSeed,setPort,hopen,bus.client,heartbeat.client,backfill.rdb,berror.client",
        "arg": {
          "setPort": "%basePort% + 2"
        }
      },

      "tick": {
        "library": "randomSeed,setPort,hopen,bus.client,heartbeat.client,backfill.tick,berror.client",
        "arg": {
          "setPort": "%basePort% + 3"
        }
      },

      "replay": {
        "library": "randomSeed,setPort,hopen,bus.client,heartbeat.client,backfill.replay,berror.client",
        "arg": {
          "setPort": "%basePort% + 4"
        }
      }

    }
  }

}